<div>
    <ol>
        <% for (let i=0; i < 20; i++) { %>
            
            <li class="citlist">
                <% if (citizens[i]) { %>
                    <a href="/citizens/<%= citizens[i]._id %>">
                        <%= citizens[i].name %>
                    </a>
                    <% } %>       </li>
        
            <% } %>
    </ol>
</div>

